package com.hummingbird.maaccount.vo;

public class LoginWithPaymentCodeVO {
	/*"reset":{
	    "mobileNum":"13912345678","smsCode":"123456", "paymentCodeDES":"DES(12345678)"
	}*/
	protected String mobileNum;
	protected String smsCode;
	protected String paymentCodeDES;
	public String toString() {
		return "VerifyPaymentCodeVO [mobileNum=" + mobileNum + ",smsCode=" + smsCode + ", paymentCodeDES=" + paymentCodeDES + "]";
	}
	public String getMobileNum() {
		return mobileNum;
	}
	public void setMobileNum(String mobileNum) {
		this.mobileNum = mobileNum;
	}
	public String getSmsCode() {
		return smsCode;
	}
	public void setSmsCode(String smsCode) {
		this.smsCode = smsCode;
	}
	public String getPaymentCodeDES() {
		return paymentCodeDES;
	}
	public void setPaymentCodeDES(String paymentCodeDES) {
		this.paymentCodeDES = paymentCodeDES;
	}
	
}
